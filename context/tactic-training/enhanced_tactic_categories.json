  "targetingHierarchy": {
    "RON": {
      "fullName": "Run of Network",
      "description": "Will run on both Mobile and Desktop, best for reaching as many people as possible, low conversions"
    },
    "AAT": {
      "fullName": "Advanced Audience Targeting",
      "description": "Audiences to Target (Ex: Adults 25-54, Households with children etc.)"
    },
    "WTG": {
      "fullName": "Website Targeting",
      "description": "Use provided website categories (Health, Business, Real Estate etc.)"
    },
    "KWD": {
      "fullName": "Keyword Targeting",
      "description": "Use keywords for targeting"
    },
    "GEO": {
      "fullName": "Geofencing With Foot Traffic",
      "description": "Radius for each fence between 1 and 18 miles as a best practice but the minimum is 100 Meters....Lookback Window needs to be added to see devices in the fences in the past (1 month, 3 months, 6 months or 1 year)"
    },
    "HLM": {
      "fullName": "Geofencing With Foot Traffic",
      "description": "Just another name for Geo-Fencing with foot traffic"
    },
    "CRM": {
      "fullName": "Audience Activation",
      "description": "Use clients CRM data for targeting and retargeting"
    },
    "RTG": {
      "fullName": "Retargeting",
      "description": "Pixels are placed inside of a GTM made in a given platform to retarget users who clicks ads"
    }
  },
